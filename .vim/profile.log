                               " (comment from TM, solution from me)
                               let modkeep= &mod
                               exe "keepj norm! o\<esc>"
                               call setline(line("."),a:x)
                               let ret= virtcol("$") - 1
                               keepj d
                               keepj norm! k
                               let &mod= modkeep
                            
                              else
                               " at least give a decent default
                               let ret= strlen(a:x)
                              endif
                            "  call Dret("s:Strlen ".ret)
                              return ret
