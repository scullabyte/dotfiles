                                else
                                 let g:netrw_dirhist_cnt= 0
                                endif
                                echo "Sorry, no predecessor directory exists yet"
                               endif
                            
                              elseif a:chg == 5
                               " U: change to the subsequent directory stored on the history list
                            "   call Decho("(user: <U>) chg to next dir from history")
